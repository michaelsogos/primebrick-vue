{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "serve",
			"problemMatcher": {
				"owner": "vue-cli",
				"fileLocation": "relative",
				"pattern": {
					"regexp": "^([^s].)((d+|d+,d+|d+,d+,d+,d+)):s+(error|warning|info)s+(TSd+)s:s*(.*)$", //^([^s].)((d+|d+,d+|d+,d+,d+,d+)):s+(error|warning|info)s+(TSd+)s:s*(.*)$
					"file": 1,
					"location": 2,
					"severity": 3,
					"code": 4,
					"message": 5
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": " DONE  Compiled successfully",
					"endsPattern": "."
				}
			},
			"isBackground": true,
			"label": "npm: serve",
			"detail": "vue-cli-service serve"
		}
	]
}
